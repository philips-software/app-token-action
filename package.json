{
  "name": "app-token-action",
  "version": "1.0.0",
  "main": "dist/index.js",
  "keywords": [
    "actions",
    "action",
    "github",
    "app"
  ],
  "license": "MIT",
  "scripts": {
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "lint": "eslint src/**/*.ts",
