    "@semantic-release/github": "^9.2.6",
    "@semantic-release/release-notes-generator": "^12.1.0",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.11.14",
    "@vercel/ncc": "^0.38.1",
    "conventional-changelog-conventionalcommits": "^7.0.2",
    "eslint": "^8.56.0",
    "eslint-plugin-github": "^4.10.1",
    "eslint-plugin-jest": "^27.6.3",
    "jest": "^29.7.0",
    "jest-circus": "^29.7.0",
    "nock": "^13.5.1",
    "semantic-release": "^23.0.0",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
